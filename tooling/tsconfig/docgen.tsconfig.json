{
  "$schema": "../../node_modules/@ye/tsconfig/schema.json",
  "exclude": ["${configDir}/src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "paths": {
      "#*": ["${configDir}/src/index.js"],
      "#/*": ["${configDir}/src/*.js"]
    }
  }
}
