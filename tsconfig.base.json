{
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "@types/bun",
      "vite/client"
    ],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ],
    "paths": {
      "@ye/domain": [
        "./packages/domain/src/index.js"
      ],
      "@ye/domain/*": [
        "./packages/domain/src/*.js"
      ],
      "@ye/domain/test/*": [
        "./packages/domain/test/*.js"
      ],
      "@ye/env": [
        "./packages/env/src/index.js"
      ],
      "@ye/env/*": [
        "./packages/env/src/*.js"
      ],
      "@ye/env/test/*": [
        "./packages/env/test/*.js"
      ],
      "@ye/demo": [
        "./packages/demo/src/index.js"
      ],
      "@ye/demo/*": [
        "./packages/demo/src/*.js"
      ],
      "@ye/demo/test/*": [
        "./packages/demo/test/*.js"
      ]
    }
  }
}
