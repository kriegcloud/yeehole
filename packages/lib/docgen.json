{
  "$schema": "../../node_modules/@effect/docgen/schema.json",
  "exclude": ["src/internal/**/*.ts"],
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "paths": {
      "@ye/demo": ["../../../demo/src/index.js"],
      "@ye/demo/*": ["../../../demo/src/*.js"]
    }
  }
}
