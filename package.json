{
  "name": "yeet-union",
  "version": "0.1.0",
  "bun": {
    "patchedDependencies": {
      "@changesets/get-github-info@0.6.0": "patches/@changesets__get-github-info@0.6.0.patch"
    }
  },
  "packageManager": "bun@1.1.10",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@biomejs/biome": "latest",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@edge-runtime/vm": "^4.0.0",
    "@ye/build-util": "workspace:^",
    "@effect/dtslint": "latest",
    "@effect/language-service": "latest",
    "@effect/vitest": "latest",
    "@types/jscodeshift": "^0.11.11",
    "@types/node": "^22.9.3",
    "@vitest/browser": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/expect": "^2.1.4",
    "@vitest/web-worker": "^2.1.4",
    "ast-types": "^0.14.2",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "fast-check": "^3.23.1",
    "glob": "^11.0.1",
    "jscodeshift": "^17.1.2",
    "madge": "^8.0.0",
    "playwright": "^1.50.1",
    "rimraf": "^6.0.1",
    "syncpack": "^13.0.2",
    "tsx": "^4.19.2",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "@ye/tsconfig": "workspace:^",
    "@effect/docgen": "latest",
    "vitest": "^3.0.5"
  },
  "private": true,
  "resolutions": {
    "dependency-tree": "^10.0.9",
    "detective-amd": "^5.0.2",
    "detective-cjs": "^5.0.1",
    "detective-es6": "^4.0.1",
    "detective-less": "^1.0.2",
    "detective-postcss": "^6.1.3",
    "detective-sass": "^5.0.3",
    "detective-scss": "^4.0.3",
    "detective-stylus": "^4.0.0",
    "detective-typescript": "^11.1.0"
  },
  "scripts": {
    "pre-build": "bun run turbo build --filter @ye/build-util",
    "preinstall": "",
    "build": "bun run pre-build && turbo run check build",
    "changeset-publish": "bun codemod && bun lint-fix && bun build && TEST_DIST= bun vitest && changeset publish",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "check": "bun turbo run check",
    "check-recursive": "bun --filter='*' exec tsc -b tsconfig.json",
    "circular": "node scripts/circular.mjs",
    "clean": "turbo clean && rimraf rm -rf tsconfig.tsbuildinfo node_modules .turbo",
    "codegen": "bun --filter='*' --parallel run codegen",
    "codemod": "node scripts/codemod.mjs",
    "coverage": "vitest --coverage",
    "docgen": "node scripts/docgen.mjs && node scripts/docs.mjs",
    "dtslint": "node scripts/dtslint.mjs",
    "dtslint-clean": "dtslint --clean",
    "lint": "biome check . && syncpack lint",
    "lint:fix": "bun lint --write && syncpack format",
    "test": "bun turbo test"
  },
  "type": "module",
  "workspaces": [
    "packages/*",
    "tooling/*"
  ]
}
